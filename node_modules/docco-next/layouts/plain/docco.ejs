<%
  // Create the full string
  var md = ''
  md += `# ${title}\n\n`
  for (var i = 0, l = sections.length; i<l; i++) {
    var section = sections[i];
    if (section.docsText) {
      md += section.docsText
      md += '\n'
    }
    if (section.codeText) {
      md += '```\n' + section.codeText + '```\n'
      md += '\n'
    }
  }
%>
<%-md%>
